{
  "name": "cajudev/rest",
  "description": "A wrapper around slim framework that makes easy build API's",
  "keywords": [
    "rest",
    "restful",
    "api",
    "php"
  ],
  "license": "MIT",
  "homepage": "https://github.com/cajudev/rest",
  "type": "library",
  "authors": [
    {
      "name": "Richard Lopes",
      "email": "contato.richardlopes@outlook.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "Cajudev\\Rest\\": [
        "src/",
        "tests/"
      ]
    }
  },
  "require": {
    "php": "^7.4",
    "slim/slim": "4.4",
    "slim/psr7": "1.6.1",
    "doctrine/orm": "2.7.2",
    "doctrine/migrations": "2.2.1"
  },
  "require-dev": {
    "phpunit/phpunit": "9.1.1",
    "php-coveralls/php-coveralls": "2.2.0"
  }
}
